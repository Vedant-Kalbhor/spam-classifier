{
    "manifest_version": 3,
    "name": "Email Spam Classifier",
    "version": "1.0",
    "description": "A Chrome extension to classify emails as SPAM or NOT SPAM.",
    "permissions": ["storage"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "spam.png",
        "48": "spam.png",
        "128": "spam.png"
      }
    }
  }
  